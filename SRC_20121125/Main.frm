        ElseIf MapX > HScroll.max Then
            MapX = HScroll.max
        End If
        
        'Çxé≤ÇÃà⁄ìÆó ÇéZèo
        MapY = PrevMapY - (Y - PrevMouseY) \ 32
        If MapY < 1 Then
            MapY = 1
        ElseIf MapY > VScroll.max Then
            MapY = VScroll.max
        End If
        
        If MapFileName = "" Then
            'ÉXÉeÅ[É^ÉXâÊñ ÇÃèÍçáÇÕà⁄ìÆó Çå¿íË
            MapX = 8
            If MapY < 8 Then
