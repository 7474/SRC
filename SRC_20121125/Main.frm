    'ÇfÇtÇhÉçÉbÉNíÜÇÕíPÇ»ÇÈÉNÉäÉbÉNÇ∆ÇµÇƒèàóù
    If IsGUILocked Then
        If frmMessage.Visible Then
            IsFormClicked = True
        End If
        If WaitClickMode Then
            IsFormClicked = True
        End If
        Exit Sub
    End If
    
    Select Case Button
        Case 1
            'ç∂ÉNÉäÉbÉN
            PrevMapX = MapX
            PrevMapY = MapY
            PrevMouseX = X
            PrevMouseY = Y
            Select Case CommandState
                Case "É}ÉbÉvÉRÉ}ÉìÉh"
                    CommandState = "ÉÜÉjÉbÉgëIë"
                Case "ÉÜÉjÉbÉgëIë"
                    xx = PixelToMapX(X)
                    yy = PixelToMapY(Y)
                    If xx < 1 Or MapWidth < xx _
                        Or yy < 1 Or MapHeight < yy _
                    Then
                        IsDragging = True
                    ElseIf Not MapDataForUnit(xx, yy) Is Nothing Then
                        ProceedCommand
                    Else
                        IsDragging = True
                    End If
                Case "É^Å[ÉQÉbÉgëIë", "à⁄ìÆå„É^Å[ÉQÉbÉgëIë"
                    xx = PixelToMapX(X)
                    yy = PixelToMapY(Y)
                    If xx < 1 Or MapWidth < xx _
                        Or yy < 1 Or MapHeight < yy _
                    Then
                        IsDragging = True
                    ElseIf Not MaskData(xx, yy) Then
                        ProceedCommand
                    Else
                        IsDragging = True
                    End If
' MOD START MARGE
'                Case "ÉRÉ}ÉìÉhëIë", "à⁄ìÆå„ÉRÉ}ÉìÉhëIë"
                Case "ÉRÉ}ÉìÉhëIë"
' MOD  END  MARGE
                    CancelCommand
' ADD START MARGE
                    'Ç‡ÇµêVÇµÇ¢ÉNÉäÉbÉNínì_Ç™ÉÜÉjÉbÉgÇ»ÇÁÅAÉÜÉjÉbÉgëIëÇÃèàóùÇêiÇﬂÇÈ
                    xx = PixelToMapX(X)
                    yy = PixelToMapY(Y)
                    If xx < 1 Or MapWidth < xx _
                        Or yy < 1 Or MapHeight < yy _
                    Then
                        IsDragging = True
                    ElseIf Not MapDataForUnit(xx, yy) Is Nothing Then
                        ProceedCommand
                    Else
                        IsDragging = True
                    End If
                Case "à⁄ìÆå„ÉRÉ}ÉìÉhëIë"
                    CancelCommand
' ADD  END  MARGE
                Case Else
                    ProceedCommand
            End Select
        Case 2
            'âEÉNÉäÉbÉN
            Select Case CommandState
                Case "É}ÉbÉvÉRÉ}ÉìÉh"
                    CommandState = "ÉÜÉjÉbÉgëIë"
                Case "ÉÜÉjÉbÉgëIë"
                    ProceedCommand True
                Case Else
                    CancelCommand
            End Select
    End Select
End Sub

