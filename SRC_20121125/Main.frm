                MapY = MapHeight - 7
            End If
        End If
        
        'É}ÉbÉvâÊñ ÇêVÇµÇ¢ç¿ïWÇ≈çXêV
        If Not MapX = LastMapX Or Not MapY = LastMapY Then
            RefreshScreen
        End If
    End If
End Sub

