Private Sub picMain_MouseMove(Index As Integer, Button As Integer, Shift As Integer, _
    X As Single, Y As Single)
Static LastMouseX As Integer, LastMouseY As Integer
Static LastMapX As Integer, LastMapY As Integer
Static LastHostSpot As String
Dim xx As Integer, yy As Integer
Dim i As Integer
    
    'ëOâÒÇÃÉ}ÉEÉXà íuÇãLò^
    LastMouseX = MouseX
    LastMouseY = MouseY
    
    'åªç›ÇÃÉ}ÉEÉXà íuÇãLò^
    MouseX = X
    MouseY = Y
    
    'ÇfÇtÇhÉçÉbÉNíÜÅH
    If IsGUILocked Then
        If Not WaitClickMode Then
            Exit Sub
        End If
        
        'ÉzÉbÉgÉ|ÉCÉìÉgÇ™íËã`Ç≥ÇÍÇƒÇ¢ÇÈèÍçáÇÕÉcÅ[ÉãÉ`ÉbÉvÇïœçX
        For i = 1 To UBound(HotPointList)
            With HotPointList(i)
                If .Left <= MouseX And MouseX < .Left + .width _
                    And .Top <= MouseY And MouseY < .Top + .Height _
                Then
                    If .Caption = "îÒï\é¶" Or .Caption = "" Then
                        Exit For
                    End If
                    
                    If .Name <> LastHostSpot And LastHostSpot <> "" Then
                        Exit For
                    End If
                    
                    'ÉcÅ[ÉãÉ`ÉbÉvÇÃï\é¶
                    frmToolTip.ShowToolTip .Caption
                    
                    With picMain(0)
                        If .MousePointer <> 99 Then
                            .Refresh
                            .MousePointer = 99
                        End If
                    End With
                    
                    LastHostSpot = .Name
                    Exit Sub
                End If
            End With
        Next
        
        'ÉzÉbÉgÉ|ÉCÉìÉgè„Ç…ÉJÅ[É\ÉãÇ™Ç»ÇØÇÍÇŒÉcÅ[ÉãÉ`ÉbÉvÇè¡Ç∑
        frmToolTip.Hide
        LastHostSpot = ""
        picMain(0).MousePointer = 0
        Exit Sub
    End If
    
    'É}ÉbÉvÇ™ê›íËÇ≥ÇÍÇƒÇ¢Ç»Ç¢èÍçáÇÕÇ±ÇÍà»ç~ÇÃîªíËÇÕïsóv
    If MapWidth < 15 Or MapHeight < 15 Then
        Exit Sub
    End If
    
    'ÉJÅ[É\Éãè„Ç…ÉÜÉjÉbÉgÇ™Ç¢ÇÍÇŒÉXÉeÅ[É^ÉXÉEÉBÉìÉhÉEÇ…ÇªÇÃÉÜÉjÉbÉgÇï\é¶
    xx = PixelToMapX(X)
    yy = PixelToMapY(Y)
'MOD START 240a
'    If MainWidth = 15 Then
    If Not NewGUIMode Then
'MOD  END
        If 1 <= xx And xx <= MapWidth _
            And 1 <= yy And yy <= MapHeight _
        Then
'MOD START 240a
'            If Not MapDataForUnit(xx, yy) Is Nothing Then
'                InstantUnitStatusDisplay xx, yy
'            End If
            If MapDataForUnit(xx, yy) Is Nothing Then
                If Not MapFileName = "" Then
                    'ÉÜÉjÉbÉgÇ™Ç¢Ç»Ç¢ÅAÇ©Ç¬ÉXÉeÅ[É^ÉXï\é¶Ç≈Ç»ÇØÇÍÇŒínå`èÓïÒÇï\é¶
                    DisplayGlobalStatus
                End If
            Else
                InstantUnitStatusDisplay xx, yy
            End If
'MOD  END
'ADD START 240a
        Else
            'É}ÉbÉväOÇ…ÉJÅ[É\ÉãÇ™Ç†ÇÈèÍçá
            DisplayGlobalStatus
'ADD  END
        End If
    Else
'ADD ÉÜÉjÉbÉgëIëí«â¡ÅEà⁄ìÆéûÇ‡ï\é¶ 240a
'        If (CommandState = "É^Å[ÉQÉbÉgëIë" Or CommandState = "à⁄ìÆå„É^Å[ÉQÉbÉgëIë") _
'            And (SelectedCommand <> "à⁄ìÆ" _
'                And SelectedCommand <> "ÉeÉåÉ|Å[Ég" _
'                And SelectedCommand <> "ÉWÉÉÉìÉv") _
'        Then
        If (CommandState = "É^Å[ÉQÉbÉgëIë" Or CommandState = "à⁄ìÆå„É^Å[ÉQÉbÉgëIë" Or CommandState = "ÉÜÉjÉbÉgëIë") Then
            If 1 <= xx And xx <= MapWidth _
                And 1 <= yy And yy <= MapHeight _
            Then
                If Not MapDataForUnit(xx, yy) Is Nothing Then
                    Me.picMain(0).Refresh
'                    RedrawScreen
                    InstantUnitStatusDisplay xx, yy
'ADD Else
                Else
                    ClearUnitStatus
                End If
            End If
        ElseIf MouseX <> LastMouseX Or MouseY <> LastMouseY Then
            ClearUnitStatus
        End If
    End If
    
    'É}ÉbÉvÇÉhÉâÉbÉOíÜÅH
    If IsDragging And Button = 1 Then
        'Çwé≤ÇÃà⁄ìÆó ÇéZèo
        MapX = PrevMapX - (X - PrevMouseX) \ 32
        If MapX < 1 Then
